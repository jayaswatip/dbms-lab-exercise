INNER JOIN 
SQL&gt; select p.projectid,p.projectname,j.empid,j.empfname,j.emplname from project p inner
join jp j on  p.empid=j.empid; 
PROJECTID PROJECTNAME EMPID EMPFNAME  EMPLNAME 
